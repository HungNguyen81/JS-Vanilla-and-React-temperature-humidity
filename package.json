{
  "name": "temperature-humidity-widget",
  "version": "0.0.1",
  "description": "Temperature Humidity widget",
  "main": "webpack.config.js",
  "scripts": {
    "devw": "webpack-dev-server --config webpack.config.js --mode=development ",
    "dev": "webpack-dev-server --config webpack-examples.config.js",
    "dev-wrapper": "webpack-dev-server --config webpack-wrapper.config.js --env.distType wrapper",
    "build": "rimraf dist && mkdir dist && webpack --config webpack-dist.config.js  --mode=production  --env.distType web --json > ./dist/stats.json && copy resources\\index.html dist",
    "build:makefile": "make build",
    "build:js": "node scripts/build.js",
    "clean": "rimraf -rf ./build",
    "babel:cjs": "cross-env BABEL_ENV=cjs babel src --out-dir build --source-maps",
    "babel:es": "cross-env BABEL_ENV=es babel src --out-dir build/es --source-maps",
    "release": "npm run clean && npm run babel:cjs && npm run babel:es && cp README.md build/  && node scripts/release.js"
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "keywords": [
    "humidity",
    "widget"
  ],
  "author": "Renzo Mischianti",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/xreef/aurora/issues"
  },
  "homepage": "https://github.com/xreef/aurora#readme",
  "dependencies": {
    "@formatjs/intl-pluralrules": "^1.5.0",
    "@formatjs/intl-relativetimeformat": "^4.5.7",
    "@preact/compat": "0.0.2",
    "babel-polyfill": "^6.26.0",
    "classnames": "^2.2.6",
    "perfect-scrollbar": "^1.4.0",
    "preact": "^10.3.3",
    "preact-compat": "^3.19.0",
    "prop-types": "^15.7.2",
    "react": "^16.13.0",
    "react-dom": "^16.13.0",
    "react-intl": "^3.12.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.8.6",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-proposal-object-rest-spread": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.8.3",
    "@babel/preset-env": "^7.8.6",
    "@babel/preset-react": "^7.8.3",
    "@babel/runtime": "^7.8.4",
    "acorn-jsx": "^5.2.0",
    "autoprefixer-loader": "^3.2.0",
    "babel-eslint": "^10.0.3",
    "babel-loader": "^8.0.4",
    "babel-plugin-react-intl": "^3.0.1",
    "compression-webpack-plugin": "^2.0.0",
    "connected-react-router": "^4.4.1",
    "css-loader": "^1.0.1",
    "eslint": "5.6.0",
    "eslint-config-airbnb": "17.1.1",
    "eslint-config-prettier": "^6.0.0",
    "eslint-config-standard": "^13.0.1",
    "eslint-plugin-babel": "^5.3.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jest": "^23.0.4",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-node": "^9.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.14.3",
    "eslint-plugin-standard": "^4.0.0",
    "extract-loader": "^3.0.0",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^2.0.0",
    "html-loader": "^0.5.5",
